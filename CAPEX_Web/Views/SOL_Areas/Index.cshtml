        </th>
        <th>
            @Html.DisplayNameFor(model => model.AREA_STATUS)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AREA_NOMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AREA_STATUS)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AREA_ID }) |
