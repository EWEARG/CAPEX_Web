@model IEnumerable<CAPEX_Web.SOL_Areas>

@{
    ViewBag.Title = "Areas";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
